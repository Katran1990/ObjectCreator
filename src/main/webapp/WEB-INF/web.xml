<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <welcome-file-list>
        <welcome-file>start-point.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>objectCreatorServlet</servlet-name>
        <servlet-class>com.katran.objectcreator.ObjectCreatorServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>objectCreatorServlet</servlet-name>
        <url-pattern>/obj-creator</url-pattern>
    </servlet-mapping>

</web-app>